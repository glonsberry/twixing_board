<div class="header">
  <div class="login_head ten columns alpha">
    <% if current_user %>
        <p>Logged in as <%= current_user.email %></p>

        <%= link_to "LogOut", logout_path, method: :delete %>
      <% else %>
        <p><%= link_to 'Register', signup_path %></p>
        <%= link_to 'LogIn', login_path %>
    <% end %>
  </div>
  <div class="titleHead four columns omega">
    <h1 class="title">twixingboard</h1>
  </div>
  <hr>
</div>